{
  "name": "graphql-techtalk",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "npm run build:app && npm run build:server",
    "build:app": "webpack --mode=development",
    "build:server": "tsc -p ./src/server",
    "lint": "tslint -c tslint.json \"./src/{app,common,server}/**/*.{ts,tsx}\"",
    "start:server": "cd src/server && ts-node-dev index.ts && cd ../../",
    "start:app": "webpack-dev-server",
    "start": "concurrently --restart-tries 2 --names server,app -c cyan,magenta \"npm:start:server\" \"npm:start:app\"",
    "test:build": "tsc -p ./tsconfig.test.json",
    "test:run": "alsatian \"./test/**/*.test.js\"",
    "test:runci": "alsatian \"./test/**/*.test.js\" --tap | tap-spec",
    "test:rundebug": "alsatian \"./test/**/*.test.js\" --tap",
    "test:clean": "rimraf test",
    "test": "npm run test:clean && npm run test:build && npm run test:run",
    "test:ci": "npm run test:clean && npm run test:build && npm run test:runci",
    "test:debug": "npm run test:build && npm run test:rundebug",
    "coverage": "nyc --reporter=lcov --reporter=html npm run test:run && nyc report"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playlyfe/gql": "^2.6.2",
    "@types/graphql": "^14.5.0",
    "@types/graphql-date": "^1.0.5",
    "@types/mongodb": "^3.3.10",
    "@types/mongoose": "^5.5.30",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/supertest": "^2.0.8",
    "alsatian": "^3.1.0",
    "chalk": "2.4.0",
    "concurrently": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "neworbit-tslint-config": "^6.0.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "tap-spec": "^5.0.0",
    "ts-loader": "^6.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@stardust-ui/react": "^0.40.3",
    "@typegoose/typegoose": "^6.1.1",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.8",
    "graphql": "^14.5.8",
    "graphql-date": "^1.0.3",
    "graphql-tag": "^2.10.1",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.9",
    "polished": "^3.4.2",
    "react": "^16.11.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^4.4.1",
    "ts-node": "^8.5.0",
    "type-graphql": "^0.17.5",
    "typescript": "^3.7.2"
  },
  "bundleDependencies": [
    "react",
    "react-dom"
  ]
}
